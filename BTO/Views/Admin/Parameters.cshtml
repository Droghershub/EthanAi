        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">{{'Clone version' | translate}}</h4>
            </div>
            <div class="modal-body">
                <treasure-overlay-spinner active='spinner.active'>
                    <form name="cloneVersionModelForm">
                        <div class="form-group row" ng-if="errorCloneProductVersion == true">

                            <div class="col-md-10 col-sm-10 col-xs-10 bto-error" style="color:red !important">
                                {{'Name and version is duplicate!' | translate}}
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="control-label col-sm-4" for="environment_name">{{"Environment name" | translate}}</label>
                            <div class="col-sm-8">
                                <input id="environment_name" placeholder="{{'Parameter format number' | translate}}" class="form-control"
                                       name="environment_name"
                                       ng-model="environmentVersion.name" required />

                                <div class="bto-error" ng-messages="cloneVersionModelForm.environment_name.$error" ng-if="cloneVersionModelForm.environment_name.$dirty">
                                    <div ng-message="required">
                                        {{"Environment name is requried" | translate}}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="control-label col-sm-4 " for="version_name">{{"Version Name" | translate}}</label>
                            <div class=" col-sm-8">
                                <input id="version_name" placeholder="{{'Version Name' | translate}}" class=" form-control"
                                       name="version_name"
                                       ng-model="environmentVersion.version"
                                       required />
                                <div class="bto-error" ng-messages="cloneVersionModelForm.version_name.$error" ng-if="cloneVersionModelForm.version_name.$dirty">
                                    <div ng-message="required">{{"Version name is required" | translate}}</div>
                                </div>
                            </div>
                        </div>
                    </form>
                </treasure-overlay-spinner>
            </div>
            <div class="modal-footer">
                <button type="button" id="closeCloneFrom" class="btn btn-default" data-dismiss="modal">{{'Cancel' | translate}}</button>
                <button type="button" class="btn btn-primary" ng-disabled="cloneVersionModelForm.$invalid" ng-click="saveCloneNewVersion();">
                    <span>{{'Save' | translate}}</span>
                </button>
            </div>
        </div>

    </div>
</div>
